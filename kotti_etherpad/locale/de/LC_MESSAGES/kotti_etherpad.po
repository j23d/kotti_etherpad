#: kotti_etherpad/resources.py:32 kotti_etherpad/views.py:118
#: kotti_etherpad/templates/protocol_widget.pt:8
msgid "Etherpad"
msgstr "Etherpad"

#: kotti_etherpad/views.py:32
msgid "Pad ID"
msgstr "Pad ID"

#: kotti_etherpad/views.py:33
msgid "The ID to identify your Etherpad."
msgstr "ID des Etherpads"

#: kotti_etherpad/views.py:38
msgid "Domain"
msgstr "Domain"

#: kotti_etherpad/views.py:39
msgid "The domain of the server."
msgstr "Die vollständige URL des servers."

#: kotti_etherpad/views.py:42
msgid "This is not a valid domain."
msgstr ""
"Die eingegebene Domain ist nicht valide. Bitte denken Sie auch an das "
"Protokoll (http://)."

#: kotti_etherpad/views.py:47
msgid "Port"
msgstr "Port"

#: kotti_etherpad/views.py:48
msgid "Port of the server. Defaults to 80."
msgstr "Port des Servers. Normalerweise 80."

#: kotti_etherpad/views.py:53
msgid "Show controls"
msgstr "Kontrollleisten anzeigen"

#: kotti_etherpad/views.py:54
msgid "Decide if you want to show the controls."
msgstr "Zeige die Kontrollleisten im Etherpad an."

#: kotti_etherpad/views.py:59
msgid "Show chat"
msgstr "Chat anzeigen"

#: kotti_etherpad/views.py:60
msgid "Show chat window in the right column."
msgstr "Zeige das Chat Fenster in der rechten Spalte des Etherpad an."

#: kotti_etherpad/views.py:65
msgid "Show line numbers"
msgstr "Zeilennummer anzeigen"

#: kotti_etherpad/views.py:66
msgid "Show line numbers in the chat window."
msgstr "Zeige die Zeilennummern im Etherpad an."

#: kotti_etherpad/views.py:71
msgid "Monospace font"
msgstr "Konstantschrift"

#: kotti_etherpad/views.py:72
msgid "Use Monospaced fonts."
msgstr "Benutze diktengleiche Schrift."

#: kotti_etherpad/views.py:77
msgid "No colors"
msgstr "Keine Farben"

#: kotti_etherpad/views.py:78
msgid "Disable background colors on author text."
msgstr "Deaktiviere Hintergrundfarben der Texte einzelner Autoren."

#: kotti_etherpad/views.py:83
msgid "Hide QR code"
msgstr "Verstecke QR code"

#: kotti_etherpad/views.py:84
msgid "Hide QR code."
msgstr "Verstecke QR code."

#: kotti_etherpad/views.py:89
msgid "Width"
msgstr "Breite"

#: kotti_etherpad/views.py:90
msgid "The width of the embedded IFrame."
msgstr "Die Breite des eingebetteten IFrame."

#: kotti_etherpad/views.py:95
msgid "Height"
msgstr "Höhe"

#: kotti_etherpad/views.py:96
msgid "The height of the embedded IFrame."
msgstr "Die Höhe des eingebetteten IFrame."

#: kotti_etherpad/views.py:102
msgid "Border"
msgstr "Rahmen"

#: kotti_etherpad/views.py:103
msgid "The width of the border (make sure to append px to a numerical value)."
msgstr "Die Breite des Rahmens (numerische Angabe plus px)."

#: kotti_etherpad/views.py:109
msgid "Border Style"
msgstr "Stil des Rahmens"

#: kotti_etherpad/views.py:110
msgid ""
"The CSS style of the border [none, dotted, dashed, solid, double, groove,"
" ridge, inset, outset]"
msgstr ""
"Der CSS Stil des Rahmens [none, dotted, dashed, solid, double, groove,\"\n"
"\" ridge, inset, outset]."

#: kotti_etherpad/views.py:134
msgid "Anonymous user"
msgstr "Anonymer Nutzer"

#: kotti_etherpad/templates/protocol_widget.pt:5
msgid "Etherpads"
msgstr "Etherpads"

#: kotti_etherpad/templates/protocol_widget.pt:9
msgid "Protocol"
msgstr "Protokoll"

#: kotti_etherpad/templates/protocol_widget.pt:17
msgid "Timeslider"
msgstr "Zeitleiste"

